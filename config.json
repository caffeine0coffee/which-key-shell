{
  "items": [
    {
      "key": "g",
      "name": "Git...",
      "type": "bindings",
      "bindings": [
        {
          "key": "s",
          "name": "status short",
          "type": "command",
          "command": "git status -s"
        },
        {
          "key": "S",
          "name": "status",
          "type": "command",
          "command": "git status"
        },
        {
          "key": "a",
          "name": "add all",
          "type": "command",
          "command": "git add -A"
        },
        {
          "key": "c",
          "name": "commit",
          "type": "command",
          "command": "git commit"
        },
        {
          "key": "u",
          "name": "pull",
          "type": "command",
          "command": "git pull"
        },
        {
          "key": "p",
          "name": "push",
          "type": "command",
          "command": "git push"
        },
        {
          "key": "P",
          "name": "add, commit with \"update\", push",
          "type": "command",
          "command": "git add -A && git commit -m \"update\" && git push"
        },
        {
          "key": "r",
          "name": "set upstream and push",
          "type": "command",
          "command": "git push --set-upstream origin `git branch --show-current`"
        },
        {
          "key": "o",
          "name": "oneline log",
          "type": "command",
          "command": "git log --graph --decorate --oneline --abbrev-commit"
        },
        {
          "key": "l",
          "name": "log",
          "type": "command",
          "command": "git log --graph --decorate --abbrev-commit -5 --all --pretty=short"
        },
        {
          "key": "L",
          "name": "log",
          "type": "command",
          "command": "git log --graph --decorate --abbrev-commit"
        }
      ]
    },
    {
      "key": "t",
      "name": "Tmux...",
      "type": "bindings",
      "bindings": [
        {
          "key": "r",
          "name": "rename session",
          "type": "command",
          "command": "rename_tmux_session.sh"
        },
        {
          "key": "w",
          "name": "rename window",
          "type": "command",
          "command": "rename_tmux_window.sh"
        }
      ]
    },
    {
        "key": "u",
        "name": "Utils...",
        "type": "bindings",
        "bindings": [
            {
                "key": "k",
                "name": "curl github ssh pubkeys to authorized_keys",
                "type": "command",
                "command": "curl https://github.com/caffeine0coffee.keys > ~/.ssh/authorized_keys"
            },
            {
                "key": "y",
                "name": "restart yabai",
                "type": "command",
                "command": "yabai --restart-service"
            }
        ]
    }
  ]
}
